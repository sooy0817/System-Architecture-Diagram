# Example environment variables for testing

# Database
DATABASE_URL=sqlite:///./test_diagram_agent.db

# API Configuration
API_HOST=localhost
API_PORT=8001
LOG_LEVEL=DEBUG

# External Services
LANGFUSE_PUBLIC_KEY=your_public_key_here
LANGFUSE_SECRET_KEY=your_secret_key_here

# GLiNER Model
GLINER_MODEL_NAME=urchade/gliner_base

# Test Configuration
TEST_SESSION_ID=test_session_123
TEST_DATA_PATH=./test_data

# Storage
STORAGE_PATH=./test_data/storage

# Graph Database (if using external graph DB)
GRAPH_DB_URL=bolt://localhost:7687
GRAPH_DB_USER=neo4j
GRAPH_DB_PASSWORD=password

# Export Settings
EXPORT_PATH=./test_exports
TEMP_PATH=./temp

# Performance Settings
MAX_NODES=1000
MAX_EDGES=5000
BATCH_SIZE=100

# Security
SECRET_KEY=your_secret_key_for_testing
ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=30